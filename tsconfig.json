{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es5",
      "es6",
      "dom",
      "dom.iterable"
    ],
    "module": "CommonJS",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types",
      "custom_typings/"
    ]
  }
}
